      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "stylelint": {
    "ignoreFiles": "*"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "/__tests__/.*\\.(test|spec)\\.(js|tsx?)$",
    "transform": {
      "^.+\\.(js|ts|tsx)$": "babel-jest"
    },
    "setupFiles": [
      "<rootDir>/jest/resolveTypescript.js"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**",
      "!**/node_modules/**",
      "!**/__tests__/**",
      "!**/__fixtures__/**"
    ]
  },
  "browserslist": [
    "> 0.25% in US",
